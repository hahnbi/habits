{
  "name": "habits",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-router-dom": "4.3.0",
    "ajv": "6.5.2",
    "bootstrap": "4.1.3",
    "firebase": "5.3.0",
    "jquery": "3.3.1",
    "js-joda": "1.8.2",
    "node-sass-chokidar": "^1.3.3",
    "popper.js": "1.14.4",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
